NET "clk"   LOC = "PIN44";
# NET "vhost" LOC = "PIN40";
NET "pulse_in" LOC = "PIN20"; # J11-2
NET "led_d2" LOC = "PIN21"; 
NET "led_d3" LOC = "PIN22"; 

#NET "servo<0>" LOC = "PIN2"; 
